{
  "name": "magickwand",
  "version": "0.0.1",
  "description": "Node.js bindings for imagemagick",
  "main": "./lib/index.js",
  "scripts": {
    "deps:magickcore:import": "node src/deps.js src/deps-stub.cc deps/ImageMagick/MagickCore build/magickcore-import.i import",
    "deps:magickcore:include": "node src/deps.js src/deps-stub.cc deps/ImageMagick/MagickCore build/magickcore.i",
    "deps:magickcore": "npm run deps:magickcore:import && npm run deps:magickcore:include",
    "deps:magick++": "node src/deps.js src/deps-stub.cc deps/ImageMagick/Magick\\\\+\\\\+ build/magick++.i",
    "deps:magickwand": "node src/deps.js src/deps-stub.cc deps/ImageMagick/MagickWand build/magickwand.i",
    "deps": "npm run deps:magickcore && npm run deps:magick++ && npm run deps:magickwand",
    "swig": "swig -javascript -Ideps/ImageMagick/Magick++/lib -Ideps/ImageMagick -napi -c++ -o build/Magick++.cxx src/Magick++.i",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmomtchev/node-magickwand.git"
  },
  "keywords": [
    "imagemagick",
    "node.js"
  ],
  "author": "Momtchil Momtchev <momtchil@momtchev.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmomtchev/node-magickwand/issues"
  },
  "homepage": "https://github.com/mmomtchev/node-magickwand#readme",
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "node-addon-api": "^5.1.0"
  }
}
